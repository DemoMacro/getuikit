{
  "name": "@demomacro/getuikit",
  "version": "0.0.0",
  "description": "A UIKit-style front-end framework, powered by Demo Macro.",
  "private": true,
  "scripts": {
    "build": "pnpm -r prepack",
    "check": "rome check . --apply-unsafe && rome format . --write",
    "format": "prettier --write --list-different . --ignore-path .gitignore",
    "postinstall": "githooks install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DemoMacro/getuikit.git"
  },
  "keywords": [
    "uikit",
    "preset",
    "unocss",
    "front-end",
    "framework",
    "ui",
    "vue"
  ],
  "author": {
    "name": "Demo Macro",
    "email": "abc@imst.xyz",
    "url": "https://imst.xyz/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DemoMacro/getuikit/issues"
  },
  "homepage": "https://github.com/DemoMacro/getuikit#readme",
  "devDependencies": {
    "@funish/githooks": "edge",
    "@funish/githooks-config": "edge",
    "@funish/lint": "edge",
    "prettier": "3.0.1",
    "rome": "12.0.0",
    "tsup": "6.7.0",
    "unbuild": "1.2.0"
  }
}
