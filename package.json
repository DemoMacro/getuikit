{
  "name": "@demomacro/getuikit",
  "version": "0.0.0",
  "description": "A UIKit-style front-end framework, powered by Demo Macro.",
  "private": true,
  "scripts": {
    "build": "pnpm -r prepack",
    "check": "biome check . --apply-unsafe && biome format . --write",
    "format": "prettier --write --list-different . --ignore-path .gitignore",
    "postinstall": "githooks install && pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DemoMacro/getuikit.git"
  },
  "keywords": [
    "getuikit",
    "uikit",
    "preset",
    "unocss",
    "front-end",
    "framework",
    "ui",
    "vue"
  ],
  "author": {
    "name": "Demo Macro",
    "email": "abc@imst.xyz",
    "url": "https://imst.xyz/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DemoMacro/getuikit/issues"
  },
  "homepage": "https://github.com/DemoMacro/getuikit#readme",
  "devDependencies": {
    "@biomejs/biome": "1.3.1",
    "@funish/githooks": "edge",
    "@funish/githooks-config": "edge",
    "@funish/lint": "edge",
    "@types/node": "20.8.8",
    "prettier": "3.0.3",
    "tsup": "8.0.2",
    "unbuild": "2.0.0"
  }
}
